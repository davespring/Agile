<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <!--Import materialize.css-->
    <link type="text/css" rel="stylesheet" href="~/Content/materialize/css/materialize.min.css" media="screen,projection" />
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <!--Let browser know website is optimized for mobile-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=no" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>
    <nav>
        <div class="nav-wrapper #42a5f5 blue lighten-1 z-depth-3">
            <a href="/" class="brand-logo">Agile Board</a>
            <ul class="right hide-on-med-and-down">
                <li>@Html.ActionLink("Users", "ViewUsers", "Users")</li>
                <li>@Html.ActionLink("Stories", "ViewStories", "Stories")</li>
            </ul>
        </div>
    </nav>
    <div class="container">
        @RenderBody()
    </div>

    <script src="~/Scripts/jquery-3.0.0.min.js"></script>
    <script src="~/Scripts/jquery-ui-1.11.4.min.js"></script>
    <script type="text/javascript" src="~/Scripts/materialize/materialize.min.js"></script>
    <script src="~/Scripts/Site.js"></script>
    
</body>
</html>